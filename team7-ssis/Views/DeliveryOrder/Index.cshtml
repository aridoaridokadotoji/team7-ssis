@section scripts
{
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>
    <script src="https://cdn.datatables.net/select/1.2.6/js/dataTables.select.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.0/js/bootstrap-select.min.js"></script>
    <script src="~/Scripts/Custom/DeliveryOrder.js"></script>
}

<link rel="stylesheet" href="~/Content/Site.css">

<!DOCTYPE html>
<html>

<body>
    <div class="col-md-8">

        <h2>Receive Goods</h2>

    </div>
    <div style="width:90%; margin:0 auto" class="tablecontainer">

        <h3> Select the Purchase Order for which your are receiving goods</h3>
        <table id="myTable" class="hover">
            <thead>
                <tr>
                    <th></th>
                    <th>PurchaseOrderNo</th>
                    <th>Supplier Name</th>
                    <th>Order Date</th>
                    <th>Status</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
    <div>
        @using (Html.BeginForm("ReceiveGoodsView", "DeliveryOrder", FormMethod.Get, new { Id = "Form" }))
        {
        <input type="submit" name="Submit" value="Confirm" />
        }
    </div>
</body>
</html>

